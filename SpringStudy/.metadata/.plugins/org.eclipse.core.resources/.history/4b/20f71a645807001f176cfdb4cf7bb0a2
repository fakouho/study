<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.db.BoardMapper">
	  <!-- 책 목록 조회 -->
  <select id="bookList" resultType="com.yourpackage.model.BookVO">
    SELECT num, title, author, company, isbn, count FROM tbl_book
  </select>
    <insert id="bookInsert">
        INSERT INTO tbl_book (num, title, author, company, isbn, count)
        VALUES (#{num}, #{title}, #{author}, #{company}, #{isbn}, #{count});
    </insert>

</mapper>