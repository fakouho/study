package com.smhrd.myapp.entity;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class Member {
	
	private Long id;
	private String userid;
	private String pw;
	private String role; // 역할 권한 설정 때 사용 예정
						 // default값 'user'

}
